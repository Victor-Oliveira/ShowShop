<p id="notice"><%= notice %></p>

<h1>Listing Product Baskets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_baskets.each do |product_basket| %>
      <tr>
        <td><%= link_to 'Show', product_basket %></td>
        <td><%= link_to 'Edit', edit_product_basket_path(product_basket) %></td>
        <td><%= link_to 'Destroy', product_basket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product basket', new_product_basket_path %>
